    "type": "project",
    "authors": [
        {
            "name": "Francesco Cocchianella",
            "email": "francesco@fmvisual.com"
        }
    ],
    "require": {
        "php": "^5.6",
        "symfony/console": "^3.3",
        "symfony/yaml": "^3.3",
        "symfony/dependency-injection": "^3.3",
        "symfony/config": "^3.3"

    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
